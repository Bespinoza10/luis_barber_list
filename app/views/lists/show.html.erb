<div class="container">
  <div class="row">
    <div class="col-md-10 list center-block">
      <img src="/assets/logo-list.png" alt="" class="logo-img center-block">
      <br>
      <div class="col-md-6 center-block names panel-default panel">
        <h1><%= @list.name %></h1>
        <p><strong>Barber:</strong> <%= @list.barber.name %></p>
        <hr>
      </div>

    </div>
  </div>
</div>



<%= link_to "edit", edit_list_path(@list)%>
<%= link_to "Delete", list_path(@list), method: :delete, data: {confirm: "Are you sure you want to delete this client?"}%>
